<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.tramshedtech.eventmanagement.mapper.EventMapper">
    <insert id="add" useGeneratedKeys="true" keyProperty="id">
        INSERT INTO events(title,date,time,site,room,speaker,responsible,staff,company,contact,email,av,note,tag)
            VALUES (#{title},#{date},#{time},#{site},#{room},#{speaker},#{responsible},#{staff},#{company},#{contact},#{email},#{av},#{note},#{tag})
    </insert>
</mapper>
